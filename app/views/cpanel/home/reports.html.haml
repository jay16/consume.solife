%table.table.table-bordered.table-condensed
  %thead
    %tr
      %th Date
      %th Timestamp
      %th Size
      %th View
  %tbody
    - @reports.each do |report|
      %tr
        %td= number_to_human_date(report[1])
        %td= number_to_human_time(report[2])
        %td= number_to_human_size(File.size(report[0]), precision: 0, separator: ',')
        %td= link_to "View", cpanel_report_path(timestamp: "%s_%s" % report.last(2)), target: "_blank"
